{
    "build": {
        "commands": {
            "build": "npm run dist"
        },
        "outputDirectory": "dist"
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/"
        }
    ]
}